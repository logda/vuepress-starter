#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/caiyida/代码/02_代码_个人/20240823-YidasBlog/vuepress-starter/node_modules/.pnpm/vite@5.0.13/node_modules/vite/bin/node_modules:/Users/caiyida/代码/02_代码_个人/20240823-YidasBlog/vuepress-starter/node_modules/.pnpm/vite@5.0.13/node_modules/vite/node_modules:/Users/caiyida/代码/02_代码_个人/20240823-YidasBlog/vuepress-starter/node_modules/.pnpm/vite@5.0.13/node_modules:/Users/caiyida/代码/02_代码_个人/20240823-YidasBlog/vuepress-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/caiyida/代码/02_代码_个人/20240823-YidasBlog/vuepress-starter/node_modules/.pnpm/vite@5.0.13/node_modules/vite/bin/node_modules:/Users/caiyida/代码/02_代码_个人/20240823-YidasBlog/vuepress-starter/node_modules/.pnpm/vite@5.0.13/node_modules/vite/node_modules:/Users/caiyida/代码/02_代码_个人/20240823-YidasBlog/vuepress-starter/node_modules/.pnpm/vite@5.0.13/node_modules:/Users/caiyida/代码/02_代码_个人/20240823-YidasBlog/vuepress-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
