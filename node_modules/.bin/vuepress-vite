#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/caiyida/代码/02_代码_个人/20240823-YidasBlog/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.7_vue@3.4.38/node_modules/vuepress/bin/node_modules:/Users/caiyida/代码/02_代码_个人/20240823-YidasBlog/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.7_vue@3.4.38/node_modules/vuepress/node_modules:/Users/caiyida/代码/02_代码_个人/20240823-YidasBlog/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.7_vue@3.4.38/node_modules:/Users/caiyida/代码/02_代码_个人/20240823-YidasBlog/vuepress-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/caiyida/代码/02_代码_个人/20240823-YidasBlog/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.7_vue@3.4.38/node_modules/vuepress/bin/node_modules:/Users/caiyida/代码/02_代码_个人/20240823-YidasBlog/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.7_vue@3.4.38/node_modules/vuepress/node_modules:/Users/caiyida/代码/02_代码_个人/20240823-YidasBlog/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.7_vue@3.4.38/node_modules:/Users/caiyida/代码/02_代码_个人/20240823-YidasBlog/vuepress-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vuepress/bin/vuepress-vite.js" "$@"
else
  exec node  "$basedir/../vuepress/bin/vuepress-vite.js" "$@"
fi
